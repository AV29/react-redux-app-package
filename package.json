{
  "name": "anton-vlasik-react-test-component",
  "version": "0.0.9",
  "description": "A small application built with react-redux to test it's embedding into other applications",
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "homepage": "https://github.com/AV29/react-redux-app-package.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/AV29/react-redux-app-package.git"
  },
  "scripts": {
    "open:src": "babel-node tools/serverDev.js",
    "start": "npm run open:src",
    "clean:dist": "npm run remove:dist && mkdir dist",
    "clean:umd": "npm run remove:umd",
    "clean:es": "npm run remove:es",
    "clean:min": "npm run remove:min",
    "remove:dist": "rimraf ./dist",
    "remove:umd": "rimraf ./dist/umd",
    "remove:es": "rimraf ./dist/es",
    "remove:min": "rimraf ./dist/min",
    "copy:data": "cpx ./dist/min/styles.css ./dist && ncp ./src/assets ./dist/es/assets && npm run clean:min",
    "prebuild:min": "npm run clean:min",
    "build:min": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --bail",
    "prebuild:umd": "npm run clean:umd",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.config.umd.js --bail",
    "prebuild:es": "npm run clean:es",
    "build:es": "babel ./src --out-dir ./dist/es --source-maps --presets stage-1,react --plugins babel-plugin-add-module-exports,babel-plugin-transform-react-remove-prop-types,babel-plugin-transform-runtime",
    "prebuild": "npm run clean:dist",
    "build": "npm run build:min && npm run build:es && npm run build:umd && npm run copy:data",
    "prepublish": "npm run build",
    "ultimate:dev": "npm install && npm start",
    "ultimate": "npm install && npm run build"
  },
  "author": "Anton Vlasik <snumber29@gmail.com>",
  "license": "ISC",
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "~7.1.6",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.0.3",
    "babel-helper-builder-react-jsx": "~6.26.0",
    "babel-helper-define-map": "~6.26.0",
    "babel-helper-regex": "~6.26.0",
    "babel-loader": "~7.1.2",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-transform-imports": "~1.4.1",
    "babel-plugin-transform-react-remove-prop-types": "~0.4.13",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-1": "~6.24.1",
    "babel-register": "~6.26.0",
    "chalk": "~1.1.3",
    "compression": "~1.6.2",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.3",
    "css-loader": "~0.28.7",
    "eventsource-polyfill": "~0.9.6",
    "express": "~4.16.2",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~1.1.6",
    "html-webpack-plugin": "~2.30.1",
    "less": "~2.7.3",
    "less-loader": "~4.0.5",
    "ncp": "^2.0.0",
    "open": "0.0.5",
    "postcss": "~6.0.15",
    "postcss-flexbugs-fixes": "~3.2.0",
    "postcss-loader": "~2.0.9",
    "prop-types": "~15.6.0",
    "react": "~16.2.0",
    "react-dom": "~16.1.0",
    "react-svg-loader": "~2.1.0",
    "rimraf": "~2.6.2",
    "style-loader": "~0.19.1",
    "url-loader": "~0.6.2",
    "webpack": "~3.8.1",
    "webpack-dev-middleware": "~1.12.2",
    "webpack-hot-middleware": "~2.20.0"
  },
  "dependencies": {
    "babel-polyfill": "~6.26.0",
    "classnames": "~2.2.5",
    "immutable": "~3.8.2",
    "react-highlight-words": "~0.10.0",
    "react-redux": "~5.0.6",
    "react-select": "~1.2.1",
    "redux": "~3.7.2",
    "redux-thunk": "~2.2.0",
    "reselect": "~3.0.1"
  },
  "peerDependencies": {
    "react": "~16.2.0",
    "react-dom": "~16.2.0"
  }
}
